{
  "expo": {
    "name": "DevRadar",
    "slug": "DevRadar",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7d40e7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gabrielbriks.devradar",
      "config": {
        "googleMapsApiKey": "<key>api maps key</key>"
      }
    },
    "android":{
      "package": "com.gabrielbriks.devradar",
      "config":{
        "googleMaps": {
          "apiKey": "<key>api maps key</key>"
        }
      }
    }
  }
}